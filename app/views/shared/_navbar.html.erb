<div class="navbar navbar-expand-sm navbar-lewagon">
<%= link_to root_path, class: "navbar-brand" do %>
      <img id="header-img" src="data:image/svg+xml;base64," /></h1>
  <% end %>

  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>


  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav ml-auto">
      <li class="nav-item active">
        <%= link_to "Home", root_path, class: "nav-link" %>
      </li>
      <% if current_user&.owner %>
      <% if current_user.stores.any? %>
      <li class="nav-item dropdown">
        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">My Store</a>
        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown" style="background-color: #353B48">
          <%= link_to "View My Store",store_path(current_user.stores.first), class: "dropdown-item"%>
          <%= link_to "Create a New Store", new_store_path, class: "dropdown-item" %>
          <%= link_to "All Stores", stores_path, class: "dropdown-item" %>
          <div class="dropdown-divider"></div>
          <%= link_to "Orders", orders_store_path(current_user.stores.first), class: "dropdown-item" %>
        </div>
      </li>
      <% end %>
      <% else current_user %>
      <li class="nav-item">
        <%= link_to "My Feed", stores_path, class: "nav-link" %>
      </li>
      <% end %>
      <li class="nav-item">
        <% if user_signed_in? %>
        <%= link_to "Sign Out", destroy_user_session_path, method: :delete, data: { confirm: "Are you sure?" }, class: "nav-link" %>
        <% sum = 0 %>
        <% discounted_sum = 0 %>
        <% current_user.orders.where(status: ["accepted", "collected"]).each do |order| %>
        <% sum += order.item.price * order.quantity %>
        <% discounted_sum += order.item.discounted_price * order.quantity%>
        <% end %>
        <li class="nav-item">
         <p id="brand-color" class="nav-link" >Total savings <%= number_to_currency( sum - discounted_sum, unit: "Â£") %> </p>
       </li>
       <% else %>
       <%= link_to "Sign In", new_user_session_path, class: "nav-link" %>
       <% end %>
     </li>
     <li class="nav-item dropdown">
        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i id="brand-color" class="fas fa-globe-americas" style="font-size: 20px"></i></a>
        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown" style="background-color: #353B48">
          <div class="width text-center">
            <p style="color: white">Reducing food waste lowers your carbon foot print</p>
          </div>
          <div class="width text-center">
            <p style="color: white">Earn your 2nd <em id="green">Green World</em> by completing 5 orders</p>
          </div>
          <div class="width">
            <div class="progress" style="width: 90%">
              <div class="progress-bar progress-green" role="progressbar" style="width: 40%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
            </div>
          </div>
          <div class="width text-center">
            <p><i id="green" class="fas fa-globe-americas nav-link" style="font-size: 30px"></i><i id="grey" class="fas fa-globe-americas nav-link" style="font-size: 30px"></i><i id="grey" class="fas fa-globe-americas nav-link" style="font-size: 30px"></i><i id="grey" class="fas fa-globe-americas nav-link" style="font-size: 30px"></i><i id="grey" class="fas fa-globe-americas nav-link" style="font-size: 30px"></i></p>
          </div>
        </div>
    </li>
  </ul>
</div>
</div>
